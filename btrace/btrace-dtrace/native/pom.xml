<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>btrace-dtrace</artifactId>
        <groupId>com.sun.btrace</groupId>
        <version>1.3-SNAPSHOT</version>
    </parent>

    <artifactId>dtrace-native</artifactId>
    <packaging>pom</packaging>
    <name>native</name>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <libname>libbtrace</libname>
        <compilerExecutable>cc</compilerExecutable>
    </properties>
    
    <profiles>
        <profile>
            <id>amd64</id>
            <activation>
                <os>
                    <name>SunOS</name>
                    <arch>x86_64</arch>
                </os>
            </activation>
            <modules>
                <module>native-amd64</module>
            </modules>
        </profile>
        <profile>
            <id>i386</id>
            <activation>
                <os>
                    <name>SunOS</name>
                    <arch>x86</arch>
                </os>
            </activation>
            <modules>
                <module>native-i386</module>
            </modules>
        </profile>
    </profiles>
    
</project>